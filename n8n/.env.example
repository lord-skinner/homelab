# n8n Environment Configuration Template
# Copy this file to .env and fill in your actual values
# The .env file is gitignored and will not be committed to version control

# n8n Database Configuration
N8N_POSTGRES_DB=n8n
N8N_POSTGRES_USER=n8n
N8N_POSTGRES_PASSWORD="your-secure-password-here"
POSTGRESDB_PORT=5432

# n8n Encryption Key (generate a secure random string)
N8N_ENCRYPTION_KEY="your-encryption-key-here"

# PostgreSQL Configuration
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD="your-secure-password-here"

# Example commands to generate secure values:
# Password: openssl rand -base64 32
# Encryption Key: openssl rand -hex 16